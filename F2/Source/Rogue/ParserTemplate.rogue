module F2

augment RogueGenerator
  PROPERTIES
    template_Parser =
      @|# Generated by Froley. Will not be overwritten; customize as desired.
       |$module
       |$include "$ClassNameCore.rogue"
       |$include "Scanner.rogue"
       |
       |class $ClassName : $ClassNameCore
       |  METHODS
       |$UserMethods
       |endClass
endAugment

