module F2

augment RogueGenerator
  PROPERTIES
    template_Scanner =
      @|# Originally generated by Froley. Will not be overwritten; customize as desired.
       |$module
       |$include "$ClassNameCore.rogue"
       |$include "$ModuleNameCompileError.rogue"
       |$include "$Token.rogue"
       |$include "$TokenType.rogue"
       |
       |class $ClassName : $ClassNameCore
       |endClass
endAugment

