# Scans A,B tokens
tokens AB
SYMBOL_A A
SYMBOL_A a
SYMBOL_B B
SYMBOL_B b

scanner

+ scan_token
  consume_whitespace
  if (not hasAnother) halt

  scan
    produceAny AB
  endScan
  syntaxError

+ read_id

+ consume_whitespace
  while (hasAnother)
    if (not (consume(' ' | '\r' | '\t'))) return
  endWhile
  return

